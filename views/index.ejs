<!DOCTYPE html>
<html>

<head>
  <% include partials/head %>
</head>

<body>

  <header>
    <% include partials/header %>
  </header>

  <h2>Records</h2>

  <div class="album-list">
    <% albums.forEach(function(album) { %>
      <div>
        <h5>
          <a href="/albums/<%= album.id %>">
            <%= album.title %>
          </a>
        </h5>
        <span><%= album.artist %></span>
      </div>
      <% }) %>
  </div>

  <footer class="footer">
    <% include partials/footer %>
  </footer>

</body>

</html>
